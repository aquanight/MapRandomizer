{
  "$schema": "../../schema/scenarios.schema.json",
  "scenarios": [
    {
      "name": "Partial Refill Energy",
      "globalState": {
        "maxEnergy": 199
      },
      "startRoomId": 0,
      "startNodeId": 1,
      "endRoomId": 0,
      "endNodeId": 2,
      "endState": {
        "energy": {"remaining": 150}
      }
    },
    {
      "name": "Ineffective Refill",
      "globalState": {
        "maxEnergy": 199
      },
      "startRoomId": 0,
      "startNodeId": 1,
      "startState": {
        "energy": {"remaining": 160}
      },
      "endRoomId": 0,
      "endNodeId": 2,
      "endState": {
        "energy": {"remaining": 160}
      }
    },
    {
      "name": "Refill to Full Energy",
      "startRoomId": 0,
      "startNodeId": 1,
      "endRoomId": 0,
      "endNodeId": 2,
      "endState": {
        "energy": {"remaining": 99}
      }
    },
    {
      "name": "Refill to Max Energy",
      "globalState": {
        "poolMaxEnergy": 99
      },
      "startRoomId": 0,
      "startNodeId": 1,
      "endRoomId": 0,
      "endNodeId": 2,
      "endState": {
        "energy": {"consumed": 0}
      }
    },
    {
      "name": "Reserves",
      "globalState": {
        "maxReserves": 100
      },
      "startRoomId": 0,
      "startNodeId": 1,
      "endRoomId": 0,
      "endNodeId": 2,
      "endState": {
        "energy": {"remaining": 99},
        "reserves": {"remaining": 51}
      }
    },
    {
      "name": "Full Reserves",
      "globalState": {
        "maxReserves": 50
      },
      "startRoomId": 0,
      "startNodeId": 1,
      "endRoomId": 0,
      "endNodeId": 2,
      "endState": {
        "energy": {"remaining": 99},
        "reserves": {"remaining": 50}
      }
    }
  ]
}